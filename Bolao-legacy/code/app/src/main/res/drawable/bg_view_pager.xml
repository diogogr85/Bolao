<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:start="-3dp"
        android:end="-2dp"
        android:top="-2dp">

        <shape
            android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="@color/colorDivider"
                android:dashGap="8dp"
                android:dashWidth="6dp"/>
        </shape>
    </item>

</layer-list>